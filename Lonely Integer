
import java.util.Scanner;

public class cla {
public static void main(String[] args) {
	Scanner in = new Scanner(System.in);
	int N=in.nextInt();
if((N>0&&N<100)&&(N%2==1)){
	int[] arr = new int[N];
	int x =0;
	for(int i=0;i<N;i++){
	     arr[i]=in.nextInt();
	     if(arr[i]<101)
	     {x=x^arr[i];}
	}
	    
	System.out.println(x);
}}}



